  } */
  
  /* 左侧导航布局 end*/
  
  .right-box {
    padding: 12rpx 0rpx 0 200rpx;
  
    padding-bottom: env(safe-area-inset-bottom);
    box-sizing: border-box;
  }
  
  .page-view {
    width: 100%;
    overflow: hidden;
  }
  
  .swiper {
    width: 100%;
    height: 220rpx;
    border-radius: 12rpx;
    overflow: hidden;
    transform: translateZ(0);
  }
  
  .swiper .wx-swiper-dot {
    width: 8rpx;
    height: 8rpx;
    display: inline-flex;
    background: none;
    justify-content: space-between;
  }
  
  .swiper .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    background: rgba(255, 255, 255, 0.8);
    border-radius: 16rpx;
    overflow: hidden;
  }
  
  .swiper .wx-swiper-dot-active::before {
    background: #fff;
  }
  
  .swiper .wx-swiper-dot.wx-swiper-dot-active {
    width: 16rpx;
  }
  
  .slide-image {
    width: 100%;
    height: 250rpx!important;
  }
  
  .class-box {
    /* padding-top: 30rpx; */
  }
  
  .class-item {
    background: #fff;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx;
    margin-bottom: 20rpx;
    border-radius: 12rpx;
  }
  
  .class-name {
    font-size: 22rpx;
  }
  
  .g-container {
    /* padding-top: 20rpx; */
    display: flex;
    display: -webkit-flex;
    justify-content: flex-start;
    flex-direction: row;
    flex-wrap: wrap;
  }
  
  .g-box {
    width: 50%;
    text-align: center;
    padding-top: 40rpx;
    padding: 4%;

  }
  .g-box.g-box-bill {
    width: 50%;
    text-align: center;
    padding-top: 20rpx;
  }
  
  .g-image {
    width: 200rpx;
    height: 120rpx;
  }
  .g-image.g-bill-img {
    width: 200rpx;
    height: 320rpx;
  }
  
  .g-title {
    font-size: 22rpx;
    text-align: center;
  }
  
  .first{
    display: block;
    height:68px
  }
  .first icon{
    display: block;
    font-size: 24px;
  }
  .category-title{
      font-size: 14px;
