/*轮播*/


.page-section{
width: 100%;
position: relative;
top:-10px;
}
.page-section_center{
display: flex;
flex-direction: column;
align-items: center;
}
.page-section:last-child{
margin-bottom: 0;
}
.page-section-gap{
box-sizing: border-box;
}
.page-section-spacing{
box-sizing: border-box;
}

.page-section-gap .page-section-title{
padding-left: 0;
padding-right: 0;
}

.swiper swiper{
height: 68px;
}

.swiper-item{
display: block;
height: 68px;
}
.swiper-item image{
height: 68px;
width: 100%;
}
.area-banner-02{
background-image: linear-gradient(to right,#F55F7B, #EC313A);
border-radius: 6px;
}
.area-banner-02.blue{
  background-image: linear-gradient(to right,#77C3FF, #066CDF);
  border-radius: 6px;
}
.area-banner-02 .img-left{
padding: 26rpx;
}
.area-banner-02 .news-right {
color: #fff;
padding: 30rpx 26rpx 30rpx 0 ;
}
.area-banner-02 .title {
font-size: 14px;
margin-bottom: 5px;
overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  word-break: break-all; /* 追加这一行代码 */
  margin-top: 8px;
} 
.area-banner-02 .sub {
font-size: 12px;
overflow : hidden;
text-overflow: ellipsis;
display: -webkit-box;
-webkit-line-clamp: 1;
-webkit-box-orient: vertical;
word-break: break-all; /* 追加这一行代码 */
max-height: 20px;


} 
.area-banner-02 .sub .wxParse-p{
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  word-break: break-all;
  background: none!important;
  color: #fff!important;
  text-indent: 0!important;
  font-size: 24rpx!important;
}
.area-banner-02 .date {
font-size: 12px;
text-align: right;
} 
.area-banner-list{
  /* background-image: linear-gradient(to right,#786EF1, #9D94F9); */
  border-radius: 6px;
  margin-bottom: 16px;
  height: 100%;
  width: 100%;
  max-height: 100%;
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
    background-image:url('data:image/png;base64,');


}

.area-banner-list:nth-child(2){
   background-image:url('data:image/png;base64,');

}
.area-banner-list:nth-child(3){
   background-image:url('data:image/png;base64,') ;
  
}
.area-banner-list .img-left{
  padding: 48rpx 0rpx 48rpx 40rpx;
}
.area-banner-list .img-left icon{
  font-size: 48px;
  color: #fff;
}
.area-banner-list .news-right {
  color: #fff;
  padding: 76rpx 26rpx 76rpx 0 ;
}
.area-banner-list .title {
  font-size: 18px;
  

} 
.area-banner-list .sub {
  font-size: 16px;
 
} 
.area-banner-list .more {
  text-align: center;
  padding-top: 48px;
  
} 
